#include<stdio.h>
#include<conio.h>
void main(){
    
    int a[]={62,31,65,19,3};
    int l=sizeof(a)/sizeof(a[0]);
    int b[l];
    int c[l];
    for(int i=0;i<l;i++){
        for(int j=0;j<l;j++){
            if(a[i]>a[j]){
                b[i]+=1;
            }
        }
    }
 for(int i=0;i<l;i++){
       c[b[i]]=a[i];
    }
     for(int i=0;i<l;i++){
       printf("%d \t",c[i]); 
    }
}
